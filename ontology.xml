    <!-- http://www.TheOn.org#RitualAction -->


    <owl:Class rdf:about="http://www.TheOn.org#RitualAction">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/drammar#Action"/>
        <rdfs:comment>The class RitualAction refers to all the actions describing the ways in which humans turn to divine entities in the tragedies by means of rituals.</rdfs:comment>
        <rdfs:label>RitualAction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.TheOn.org#Sacrifice -->


    <owl:Class rdf:about="http://www.TheOn.org#Sacrifice">
